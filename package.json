{
  "name": "github-snooze",
  "version": "1.5.0",
  "description": "Snooze and get notified later on your GitHub Issues and Pull Requests",
  "author": "Davide Roffo <davide.roffo@nearform.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nearform/github-snooze-chrome-extension.git"
  },
  "bugs": {
    "url": "https://github.com/nearform/github-snooze-chrome-extension/issues"
  },
  "scripts": {
    "lint": "eslint .",
    "prepare": "husky install",
    "build": "vite build",
    "build:release": "cross-env NODE_ENV=production vite build",
    "start": "vite",
    "update-manifest": "node ./scripts/updateManifest.js",
    "test": "cross-env TZ=UTC jest",
    "storybook": "start-storybook -p 6006",
    "test-storybook": "test-storybook",
    "test-storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npm run build-storybook --quiet && npx http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && npm run test-storybook\"",
    "build-storybook": "build-storybook --quiet",
    "deploy-storybook": "storybook-to-ghpages",
    "storycap:standalone": "npx storycap http://localhost:6006 --serverTimeout 120000 -V 1280x800",
    "storycap": "run-p storybook storycap:standalone"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.9",
    "@mui/lab": "^5.0.0-alpha.104",
    "@mui/material": "^5.10.10",
    "@vitejs/plugin-react": "^2.1.0",
    "chrono-node": "^2.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "uuid": "^9.0.0",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.12.10",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@crxjs/vite-plugin": "^1.0.14",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-webpack4": "^6.5.10",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack4": "^6.5.10",
    "@storybook/react": "^6.5.12",
    "@storybook/storybook-deployer": "^2.8.16",
    "@storybook/test-runner": "^0.9.0",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/react": "^13.4.0",
    "@types/chrome": "^0.0.199",
    "@types/firefox-webext-browser": "^94.0.1",
    "@types/jest": "^29.2.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^29.2.1",
    "jest-chrome": "^0.7.0",
    "jest-environment-jsdom": "^29.2.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.27.1",
    "prettier": "^2.2.1",
    "prop-types": "^15.8.1",
    "storycap": "^3.1.9",
    "styled-jsx": "^5.1.0",
    "vite": "^3.1.8",
    "vite-plugin-zip-pack": "^1.0.4"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --cache --fix"
  }
}
