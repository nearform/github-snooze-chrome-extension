{
  "name": "github-snooze",
  "version": "1.9.3",
  "description": "Snooze and get notified later on your GitHub Issues and Pull Requests",
  "author": "Davide Roffo <davide.roffo@nearform.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nearform/github-snooze-chrome-extension.git"
  },
  "bugs": {
    "url": "https://github.com/nearform/github-snooze-chrome-extension/issues"
  },
  "scripts": {
    "lint": "eslint .",
    "prepare": "husky install",
    "build": "vite build",
    "build:release": "cross-env NODE_ENV=production vite build",
    "start": "vite",
    "update-manifest": "node ./scripts/updateManifest.js",
    "test": "cross-env TZ=UTC jest",
    "storybook": "node --openssl-legacy-provider ./node_modules/.bin/start-storybook -p 6006",
    "test-storybook": "node --openssl-legacy-provider ./node_modules/.bin/test-storybook --url http://127.0.0.1:6006",
    "test-storybook:ci": "node ./storybook-test-runner.js",
    "build-storybook": "node --openssl-legacy-provider ./node_modules/.bin/build-storybook --quiet",
    "deploy-storybook": "storybook-to-ghpages",
    "storycap:standalone": "npx storycap http://localhost:6006 --serverTimeout 120000 -V 1280x800",
    "storycap": "run-p storybook storycap:standalone"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/lab": "^5.0.0-alpha.123",
    "@mui/material": "^5.11.13",
    "@urql/exchange-auth": "^2.0.0",
    "@vitejs/plugin-react": "^3.1.0",
    "chrono-node": "^2.5.1",
    "graphql": "^16.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "urql": "^3.0.4",
    "uuid": "^9.0.0",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.12.10",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@crxjs/vite-plugin": "^1.0.14",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack4": "^6.5.16",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack4": "^6.5.10",
    "@storybook/react": "^6.5.16",
    "@storybook/storybook-deployer": "^2.8.16",
    "@storybook/test-runner": "^0.9.4",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/react": "^14.0.0",
    "@types/chrome": "^0.0.224",
    "@types/firefox-webext-browser": "^111.0.0",
    "@types/jest": "^29.2.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "babel-loader": "^9.1.2",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-chrome": "^0.8.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.31.2",
    "prettier": "^2.8.5",
    "prop-types": "^15.8.1",
    "storycap": "^4.0.0",
    "styled-jsx": "^5.1.2",
    "vite": "^4.1.4",
    "vite-plugin-zip-pack": "^1.0.5"
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --cache --fix"
  }
}
