    "jest-environment-jsdom": "^29.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.2.1",
    "rollup": "^2.78.1",
    "rollup-plugin-chrome-extension": "^3.5.6",
    "rollup-plugin-empty-dir": "^1.0.4",
    "rollup-plugin-zip": "^1.0.1",
