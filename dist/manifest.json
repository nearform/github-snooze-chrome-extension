{
  "manifest_version": 3,
  "name": "GitHub Snooze",
  "version": "1.0.0",
  "description": "DEVELOPMENT build with simple auto-reloader\n[2022-07-28 11:53:01] waiting for changes...",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "GitHub Snooze"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "background",
    "scripting"
  ]
}